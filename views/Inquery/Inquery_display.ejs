<%-include('header')-%>
<div class="main-panel">
    <div class="content-wrapper">
      <div class="page-header">
        <h3 class="page-title" style="margin-left:200px;padding-top:20px">
          <b>Inquery Table</b>
        </h3>
        <a href="/inquery/registration"><button style=" margin-top: 20px;margin-right: 100px"class="btn btn-rounded btn-fw btn-gradient-primary btn-fw">Add New</button> </a>
        </div>
        <div class="col-lg-12 stretch-card">
          <div class="card">
            <div class="card-body">
              <div class="table-responsive">
                    <table class="table table-bordered">
                        <thead>
                            <tr>
                                <th>Index</th>
                                <th>inq_id</th>        
                                <th>customer_name</th>      
                                <th>inq_date</th>
                                <th>inq_question</th>     
                                <th>inq_answer</th>    
                                <th>Action</th>  
                        
                            </tr>
                        </thead>
                        <% if(inquery_array.length>0)  { %>
                        <tbody>
                            <%  for(var i=0;i<inquery_array.length;i++)  { %>
                            <tr>
                                <td><%=i+1  %></td>
                                <td><%=inquery_array[i].inq_id %></td>
                                <td><%=inquery_array[i].customer_id %></td>
                                <td><%=inquery_array[i].inq_date    %></td>
                                <td><%=inquery_array[i].inq_question %></td>
                                <td><%=inquery_array[i].inq_answer   %></td>
                            
                                <td>
                                    <a href="edit/<%=inquery_array[i]._id%>"><img title="edit" src="/images/icons/edit.png"></a>
                                    <a href="delete/<%=inquery_array[i]._id%>"><img title="delete" src="/images/icons/delete.png"></a>
                                </td>
                            </tr>
                            <% } %>
                            <%  }   else    {%>
                                <h2>No Records Found</h2>
                                <%  }   %>
                        </tbody>
                        </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <script src="../../vendors/js/vendor.bundle.base.js"></script>
    <script src="../../vendors/js/vendor.bundle.addons.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="../../js/off-canvas.js"></script>
    <script src="../../js/hoverable-collapse.js"></script>
    <script src="../../js/misc.js"></script>
    <script src="../../js/settings.js"></script>
    <script src="../../js/todolist.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <!-- End custom js for this page-->
</body>


