           <script>
            function passwordconfirm(){
            var x=document.forms["registration"]["Admin_password"].value;
            var y=document.forms["registration"]["Admin_confirm_password"].value;
            var z=document.forms["registration"]["Admin_contact_no"].value;
            
                if(x==y){
                    var phoneno = /[7-9]{1}[0-9]{9}/ ;
                        if((z.match(phoneno)))
                        {
                            alert("Registred Successfully");
                            return true;
                        }
                        else
                        {
                        alert("Contact Number should be 10 letters");
                        return false;
                        }
                    
                    }
                else{
                    alert("password don't match");
                return false;
                }
            }
            </script>
