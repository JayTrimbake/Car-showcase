<%-include('header')-%>
<div class="main-panel">
    <div class="content-wrapper">
      <div class="page-header">
        <h3 class="page-title" style="margin-left:200px;padding-top:20px">
          <b>Admin Table</b>
        </h3>
        <a href="/admin/registration"><button style=" margin-top: 20px;margin-right: 100px"class="btn btn-rounded btn-fw btn-gradient-primary btn-fw">Add New</button> </a>
        </div>
        <div class="col-lg-12 stretch-card">
          <div class="card">
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-bordered">
                  <thead>
                        <th>Admin_id</th>
                        <th>Admin Name</th>
                        <th>Contact Number</th>
                        <th>Email</th>
                        <th>Action</th>
                  </thead>
                  <% if(admin_array.length>0)  { %>
                    <tbody>
                        <%  for(var i=0;i<admin_array.length;i++)  { %>
                        <tr>
                            <td><%=i+1  %></td>
                            <td><%=admin_array[i].Admin_name%></td>
                            <td><%=admin_array[i].Admin_contact_no%></td>
                            <td><%=admin_array[i].Admin_email%></td>   
                        <td>
                            <a href="show/<%=admin_array[i]._id%>"> <img title="show" src="/images/icons/show.png"> </a>
                            <a href="edit/<%=admin_array[i]._id%>"><img title="edit" src="/images/icons/edit.png"></a>
                            <a href="delete/<%=admin_array[i]._id%>"><img title="delete" src="/images/icons/delete.png"></a>
                        </td>
                        </tr>
                        <% } %>
                        
                        <%  }   else    {%>
                            
                            <h2>No Records Found</h2>
                            
                            <%  }   %>
                    </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <script src="../../vendors/js/vendor.bundle.base.js"></script>
    <script src="../../vendors/js/vendor.bundle.addons.js"></script>
    <!-- endinject -->
    <!-- Plugin js for this page-->
    <!-- End plugin js for this page-->
    <!-- inject:js -->
    <script src="../../js/off-canvas.js"></script>
    <script src="../../js/hoverable-collapse.js"></script>
    <script src="../../js/misc.js"></script>
    <script src="../../js/settings.js"></script>
    <script src="../../js/todolist.js"></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <!-- End custom js for this page-->
  </body>
  
  
